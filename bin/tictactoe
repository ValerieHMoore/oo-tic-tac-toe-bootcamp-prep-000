#!/usr/bin/env ruby
require 'pry'
require_relative '../lib/tic_tac_toe.rb'
puts "Welcome to Tic Tac Toe!"
board = [" "," "," "," "," "," "," "," "," "]
display_board(board)
puts "Where would you like to go?"
user_input = gets.strip
index = input_to_index(user_input)
move(board,index,"X")
display_board(board)
